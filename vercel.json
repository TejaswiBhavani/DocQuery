{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/upload.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    },
    "api/search.py": {
      "runtime": "python3.11", 
      "maxDuration": 30
    },
    "api/analyze.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    },
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  }
}